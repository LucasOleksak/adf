{
	"name": "Kandidata",
	"properties": {
		"folder": {
			"name": "HRNytt"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "DataLake",
						"type": "LinkedServiceReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "ReaktionCoreEmail",
						"type": "DatasetReference"
					},
					"name": "ReaktionEmails"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "DataLake",
						"type": "LinkedServiceReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "RenameColumns"
				},
				{
					"name": "JoinWithReaktionEmailsDB"
				}
			],
			"scriptLines": [
				"parameters{",
				"     redirect_name as string ('HRN0073WW-kandidata-banner-dec-22')",
				"}",
				"source(output(",
				"          {_col0_} as integer,",
				"          {_col1_} as string,",
				"          {_col2_} as string,",
				"          {_col3_} as short,",
				"          {_col4_} as string,",
				"          {_col5_} as long",
				"     ),",
				"     useSchema: false,",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delimited',",
				"     fileSystem: 'hrnytt',",
				"     folderPath: 'stats/kandidata',",
				"     fileName: (concat($redirect_name, '.csv')),",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: false) ~> source1",
				"source(output(",
				"          id as integer,",
				"          reaktion_uid as string,",
				"          email as string,",
				"          person_id as integer,",
				"          position_id as integer,",
				"          address_id as integer,",
				"          is_company_mail as boolean,",
				"          is_private_mail as boolean,",
				"          is_optin as boolean,",
				"          is_unsubscibed as boolean,",
				"          is_active as boolean,",
				"          hash as string,",
				"          created_at as timestamp,",
				"          updated_at as timestamp,",
				"          is_bounced as boolean,",
				"          eyebeep_active as boolean,",
				"          eyebeep_source as string,",
				"          remove_from_adressgruppen as boolean,",
				"          remove_from_adressgruppen_date as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> ReaktionEmails",
				"source1 select(mapColumn(",
				"          uid = {_col0_},",
				"          uuid = {_col1_},",
				"          url = {_col2_},",
				"          redirect_id = {_col3_},",
				"          redirect_name = {_col4_},",
				"          date = {_col5_}",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RenameColumns",
				"RenameColumns, ReaktionEmails join(uid == id,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> JoinWithReaktionEmailsDB",
				"JoinWithReaktionEmailsDB sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delimited',",
				"     fileSystem: 'hrnytt',",
				"     folderPath: 'stats/kandidata/joined',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: false,",
				"     partitionFileNames:[(concat($redirect_name, '-joined', '.csv'))],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}