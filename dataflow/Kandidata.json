{
	"name": "Kandidata",
	"properties": {
		"folder": {
			"name": "HRNytt"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "DataLake",
						"type": "LinkedServiceReference"
					},
					"name": "DataFromTracking"
				},
				{
					"dataset": {
						"referenceName": "ReaktionCoreEmail",
						"type": "DatasetReference"
					},
					"name": "ReaktionEmails"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "DataLake",
						"type": "LinkedServiceReference"
					},
					"name": "SaveToCSV"
				}
			],
			"transformations": [
				{
					"name": "RenameColumns"
				},
				{
					"name": "JoinWithReaktionEmailsDB"
				},
				{
					"name": "select1"
				},
				{
					"name": "FilterOverBlacklist"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "RemoveDuplicates"
				}
			],
			"scriptLines": [
				"parameters{",
				"     redirect_name as string ('HRN0073WW-kandidata-banner-dec-22'),",
				"     ruid as integer[] ([50844])",
				"}",
				"source(output(",
				"          {_col0_} as integer,",
				"          {_col1_} as string,",
				"          {_col2_} as string,",
				"          {_col3_} as short,",
				"          {_col4_} as string,",
				"          {_col5_} as long",
				"     ),",
				"     useSchema: false,",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delimited',",
				"     fileSystem: 'hrnytt',",
				"     folderPath: 'stats/kandidata',",
				"     fileName: (concat($redirect_name, '.csv')),",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: false) ~> DataFromTracking",
				"source(output(",
				"          id as integer,",
				"          reaktion_uid as string,",
				"          email as string,",
				"          person_id as integer,",
				"          position_id as integer,",
				"          address_id as integer,",
				"          is_company_mail as boolean,",
				"          is_private_mail as boolean,",
				"          is_optin as boolean,",
				"          is_unsubscibed as boolean,",
				"          is_active as boolean,",
				"          hash as string,",
				"          created_at as timestamp,",
				"          updated_at as timestamp,",
				"          is_bounced as boolean,",
				"          eyebeep_active as boolean,",
				"          eyebeep_source as string,",
				"          remove_from_adressgruppen as boolean,",
				"          remove_from_adressgruppen_date as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> ReaktionEmails",
				"DataFromTracking select(mapColumn(",
				"          uid = {_col0_},",
				"          uuid = {_col1_},",
				"          url = {_col2_},",
				"          redirect_id = {_col3_},",
				"          redirect_name = {_col4_},",
				"          date = {_col5_}",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RenameColumns",
				"RemoveDuplicates, ReaktionEmails join(uid == id,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     partitionBy('hash', 1),",
				"     broadcast: 'auto')~> JoinWithReaktionEmailsDB",
				"JoinWithReaktionEmailsDB select(mapColumn(",
				"          email,",
				"          date = click_date",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"derivedColumn1 filter(!in($ruid, uid),",
				"     partitionBy('hash', 1)) ~> FilterOverBlacklist",
				"RenameColumns derive(click_date = toDate(toLong(toString(date)),'yyyy-MM-dd', 'Europe/Stockholm'),",
				"          click_time = toDate(toLong(toString(date)),'HH:mm', 'Europe/Stockholm')) ~> derivedColumn1",
				"FilterOverBlacklist aggregate(groupBy(uid),",
				"     each(match(name != 'uid'), $$ = first($$)),",
				"     partitionBy('hash', 1)) ~> RemoveDuplicates",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delimited',",
				"     fileSystem: 'hrnytt',",
				"     folderPath: 'stats/kandidata/joined',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: false,",
				"     partitionFileNames:[(concat($redirect_name, '-joined', '.csv'))],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     partitionBy('hash', 1)) ~> SaveToCSV"
			]
		}
	}
}