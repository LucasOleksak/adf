{
	"name": "Kandidata_HRN0178_Leeds",
	"properties": {
		"folder": {
			"name": "HRNytt"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "DataLake",
						"type": "LinkedServiceReference"
					},
					"name": "DataFromTracking"
				},
				{
					"name": "ReaktionEmails"
				},
				{
					"dataset": {
						"referenceName": "HRNyttAllContactsToTM",
						"type": "DatasetReference"
					},
					"name": "HRNyttAllContacts"
				},
				{
					"name": "Subscriptions"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "EDRPlatform",
						"type": "LinkedServiceReference"
					},
					"name": "SaveToCSV"
				}
			],
			"transformations": [
				{
					"name": "RenameColumns"
				},
				{
					"name": "JoinWithReaktionEmailsDB"
				},
				{
					"name": "SelectOnlyEmail"
				},
				{
					"name": "FilterOverBlacklist"
				},
				{
					"name": "ConvertMongoDateToYYYMMDD"
				},
				{
					"name": "RemoveDuplicates"
				},
				{
					"name": "JoinWithHRNyttAllContacts"
				},
				{
					"name": "SelectBeforeSink"
				},
				{
					"name": "FilteeOnDomainAndActive"
				},
				{
					"name": "JoinWithActiveSubscriptions"
				}
			],
			"scriptLines": [
				"parameters{",
				"     redirect_name as string ('HRN0073WW-kandidata-banner-dec-22'),",
				"     ruid as integer[] ([50844])",
				"}",
				"source(output(",
				"          {_col0_} as integer,",
				"          {_col1_} as string,",
				"          {_col2_} as string,",
				"          {_col3_} as short,",
				"          {_col4_} as string,",
				"          {_col5_} as long",
				"     ),",
				"     useSchema: false,",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delimited',",
				"     fileSystem: 'hrnytt',",
				"     folderPath: 'stats/kandidata',",
				"     fileName: (concat('kandidata_pixel_hrn0178','.csv')),",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: false) ~> DataFromTracking",
				"source(output(",
				"          id as integer,",
				"          reaktion_uid as string,",
				"          email as string,",
				"          person_id as integer,",
				"          position_id as integer,",
				"          address_id as integer,",
				"          is_company_mail as boolean,",
				"          is_private_mail as boolean,",
				"          is_optin as boolean,",
				"          is_unsubscibed as boolean,",
				"          is_active as boolean,",
				"          hash as string,",
				"          created_at as timestamp,",
				"          updated_at as timestamp,",
				"          is_bounced as boolean,",
				"          eyebeep_active as boolean,",
				"          eyebeep_source as string,",
				"          remove_from_adressgruppen as boolean,",
				"          remove_from_adressgruppen_date as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> ReaktionEmails",
				"source(output(",
				"          Epost as string,",
				"          FirstName as string,",
				"          LastName as string,",
				"          Position as string,",
				"          Company as string,",
				"          OrgNumber as string,",
				"          Telefon as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> HRNyttAllContacts",
				"source(output(",
				"          id as integer,",
				"          email_id as integer,",
				"          domain_id as integer,",
				"          is_active as boolean,",
				"          created_at as timestamp,",
				"          updated_at as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> Subscriptions",
				"DataFromTracking select(mapColumn(",
				"          uid = {_col0_},",
				"          uuid = {_col1_},",
				"          url = {_col2_},",
				"          redirect_id = {_col3_},",
				"          redirect_name = {_col4_},",
				"          date = {_col5_}",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RenameColumns",
				"RemoveDuplicates, ReaktionEmails join(uid == id,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     partitionBy('hash', 1),",
				"     broadcast: 'auto')~> JoinWithReaktionEmailsDB",
				"JoinWithReaktionEmailsDB select(mapColumn(",
				"          uid,",
				"          click_date,",
				"          email",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelectOnlyEmail",
				"ConvertMongoDateToYYYMMDD filter(!in($ruid, uid),",
				"     partitionBy('hash', 1)) ~> FilterOverBlacklist",
				"RenameColumns derive(click_date = toDate(toLong(toString(date)),'yyyy-MM-dd', 'Europe/Stockholm')) ~> ConvertMongoDateToYYYMMDD",
				"FilterOverBlacklist aggregate(groupBy(uid),",
				"     each(match(name!='uid'), $$ = first($$)),",
				"     partitionBy('hash', 1)) ~> RemoveDuplicates",
				"SelectOnlyEmail, HRNyttAllContacts join(email == Epost,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     partitionBy('hash', 1),",
				"     broadcast: 'auto')~> JoinWithHRNyttAllContacts",
				"JoinWithActiveSubscriptions select(mapColumn(",
				"          Epost = email,",
				"          LeedDate = click_date,",
				"          FirstName,",
				"          LastName,",
				"          Position,",
				"          Company,",
				"          OrgNumber,",
				"          Telefon",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelectBeforeSink",
				"Subscriptions filter(domain_id == 11 && is_active == true()) ~> FilteeOnDomainAndActive",
				"JoinWithHRNyttAllContacts, FilteeOnDomainAndActive join(uid == email_id,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> JoinWithActiveSubscriptions",
				"SelectBeforeSink sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delimited',",
				"     fileSystem: 'edrplatform',",
				"     folderPath: 'kandidata/stats',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: false,",
				"     partitionFileNames:[(concat('kandidata-HRN10780-Leeds', '-', toString(currentDate('YYYY-mm-dd')), '.csv'))],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     header: (['Epost','LeedDate','FirstName','LastName','Position','CompanyName','OrgNumber','Telefon']),",
				"     saveOrder: 1,",
				"     partitionBy('hash', 1)) ~> SaveToCSV"
			]
		}
	}
}