{
	"name": "ProcessTrackingForCRM",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Trackings",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {createdAt_$date} as string,",
				"          {updatedAt_$date} as string,",
				"          uid as string,",
				"          url as string,",
				"          client as string,",
				"          domain_id as string,",
				"          referrer_url as string,",
				"          url_without_params as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: true,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false,",
				"     partitionBy('hash', 1)) ~> source1",
				"source1 filter(ERROR_FUNCTION('')) ~> filter1"
			]
		}
	}
}